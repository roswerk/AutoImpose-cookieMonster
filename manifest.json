{
  "manifest_version": 2,

  "name": "Erase Cookies popup",
  "description": "This extension will analyze the existence of any
  modals and containers focus on enforcing the use of cookies",
  "version": "0.1.1",
  "icons": {
   "16": "img/16.png",
   "32": "img/32.png",
   "48": "img/48.png",
   "128":"img/128.png"
  },
  "background": {
    "scripts": ["./background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions":[
    "tabs",
    "<all_urls>",
    "activeTab"]
}
